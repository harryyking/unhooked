{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { 
        "EXPO_PUBLIC_CONVEX_URL": "https://insightful-vulture-707.convex.cloud",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "${EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY_DEV}"
      },
      "ios": {
        "resourceClass": "m-medium"  
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": { 
        "EXPO_PUBLIC_CONVEX_URL": "https://acrobatic-hawk-359.convex.cloud",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "${EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY_PROD}"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": { 
        "EXPO_PUBLIC_CONVEX_URL": "https://acrobatic-hawk-359.convex.cloud",
        "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "${EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY_PROD}"
      },
      "distribution": "store",
      "ios": {
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {}
  }
}