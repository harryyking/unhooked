{
  "expo": {
    "name": "Unhooked",
    "slug": "unhooked",
    "version": "1.0.3",
    "runtimeVersion": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/unhooked-ios-icon-3.png",
    "scheme": "unhooked",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/unhooked-ios-icon-3.png",
      "resizeMode": "contain",
      "imageWidth": 100,
      "backgroundColor": "#161617"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.harryking.unhooked",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.harryking.unhooked",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "expo-apple-authentication",
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
        "expo-audio",
      
      "expo-font",
      "@react-native-google-signin/google-signin",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd0f805e-e36c-479c-b76c-041d24e8a602"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/bd0f805e-e36c-479c-b76c-041d24e8a602"
    }
  }
}
