{
  "expo": {
    "name": "Unhooked",
    "slug": "unhooked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/unhooked-ios-icon-3.png",
    "scheme": "unhooked",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/unhooked-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.harryking.unhooked",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.harryking.unhooked"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "organization": "unhooked",
          "project": "react-native",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow Unhooked to access your microphone for prayer sessions."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_CONVEX_URL": "https://insightful-vulture-707.convex.cloud",
      "EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY": "pk_test_ZW1pbmVudC1sZWVjaC00MC5jbGVyay5hY2NvdW50cy5kZXYk",  
      "router": {},
      "eas": {
        "projectId": "bd0f805e-e36c-479c-b76c-041d24e8a602"
      }
    }
  }
}