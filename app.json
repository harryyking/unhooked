{
  "expo": {
    "name": "Unhooked",
    "slug": "unhooked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/unhooked-ios-icon-3.png",
    "scheme": "unhooked",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/unhooked-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.harryking.unhooked",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      ["expo-apple-authentication"],
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
        
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow Unhooked to access your microphone for prayer sessions."
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bd0f805e-e36c-479c-b76c-041d24e8a602"
      }
    }
  }
}
